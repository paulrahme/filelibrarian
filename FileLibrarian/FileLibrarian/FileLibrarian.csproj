<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net5.0</TargetFramework>
    <AssemblyName>fl</AssemblyName>
    <AssemblyVersion>1.2.2.0</AssemblyVersion>
    <FileVersion>1.2.2.0</FileVersion>
    <Version>1.0.0</Version>
    <Authors>Paul Rahme</Authors>
    <Product>File Librarian</Product>
    <Copyright>GPL v3</Copyright>
    <PackageProjectUrl>https://github.com/paulrahme/filelibrarian</PackageProjectUrl>
    <RepositoryUrl>https://github.com/paulrahme/filelibrarian</RepositoryUrl>
    <RepositoryType>GitHub</RepositoryType>
    <Description>Command-line tool for recursive searching, sorting and grouping operations on files.</Description>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="LitJson" Version="0.17.0" />
  </ItemGroup>

</Project>
